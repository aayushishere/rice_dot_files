
! special
*.foreground:   #f4f2f0
!*.background:   #181a1b
*.background:   #171717
*.cursorColor:  #f4f2f0
*.accent: 	#18ffff

! black
*.color0:       #3d335c
*.color8:       #483c6c

! red
*.color1:       #da0f7a
*.color9:       #ff217c

! green
*.color2:       #00f58f
*.color10:      #21ffa2

! yellow
*.color3:       #ffc65c
*.color11:      #ffd486

! blue
